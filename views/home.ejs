  <body>
    <div class="container">
      <div class="row">
        <center style="margin-top: 10%;"><h1>CSV Reader</h1></center>
        <div  id="file-input" class="upload-container">
              <div class="card">
                <h3>Upload Files</h3>
                <div class="drop_box">
                  <form action="/addFile" method="post" enctype="multipart/form-data">
                    <input type="file" class="custom-file-input" value="/home/vishnubv944/csv-reader/demo.csv" id="file" name="file">
                    <label class="custom-file-label" for="inputGroupFile04"></label> <br><br>
                    <input class="btn" type="submit" />
                  </form>
                  <h5>If you are testing this website... click "Submit" button to use an existing file from the server</h5>
                </div>
            
              </div>
        </div>
    </div>



    </div>
    <script type="text/javacsript" src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
    <script src="/js/index.js"></script>
